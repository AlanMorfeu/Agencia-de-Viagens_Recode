@model CrudTripTravel.Models.NovaPasta.TBClientes
<head>
    <link rel="stylesheet" href="~/css/tt_contatos.css" />
</head>
<header> 
    <nav class="navbar navbar-light">
        <a asp-controller="Home" asp-action="Index">Home</a>
        <a asp-controller="Home" asp-action="Destinos">Destinos</a>
        <a asp-controller="Home" asp-action="Promocoes">Promoções</a>
        <a asp-controller="Home" asp-action="Contatos">Contatos</a>
        <a asp-controller="TBClientes" asp-action="Index">Crud</a>
    </nav>
</header>

<main class="container">
    <h4 class="text-center my-4">
        Obrigado pelas informações :)  ,entraremos em contato para exclarecer suas duvidas.
    </h4>
    <br />
    <br />
        <div class="divCarro text-center">
            <form asp-action="TBClientes" method="post">
                <div class="row">
                    <div class="col-12 col-sm">
                        <form action="" method="post">
                            <div class="form-group">
                                <label>Nome</label>
                                <input asp-for="nome" type="text" class="form-control" required>
                            </div>
                            <div class="form-group">
                                <label for="exampleInputPassword1">Email</label>
                                <input asp-for="email" type="email" class="form-control" required>
                            </div>
                            <div class="form-group">
                                <label for="exampleInputPassword1">Telefone</label>
                                <input asp-for="telefone" type="number" class="form-control" required>
                            </div>
                            <button type="submit" class="btnAtualizar ">Cadastrar</button>
                        </form>
                    </div>
                </div>
            </form>
        </div>
</main>
